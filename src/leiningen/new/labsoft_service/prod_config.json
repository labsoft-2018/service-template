{
  "env": "prod",
  "service-name": "{{name}}",
  "service-password": "{{password}}",
  "datomic-endpoint": "datomic:mem://{{namespace}}",
  "port": {{http-port}},
  "s3-auth": {
    "bucket-name": "labsoft-secrets",
    "region": "us-east-1"
  },
  "sqs": {
    "endpoint": "us-east-1"
  },
  "services": {
    "auth": {
      "host": "https://auth.labsoft.host",
      "endpoints": {
        "service-token": "/api/services/token"
      }
    }
  },
  "pub-key-path": "prod/pubkey.pem",
  "priv-key-path": "prod/privkey.pem"
}